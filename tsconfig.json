{
  "compilerOptions": {
    "lib": ["esnext"],
    "module": "commonjs",
    "target": "es2020",
    "outDir": "dist",
    "sourceMap": true,
    "strict": true,
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  }
}
